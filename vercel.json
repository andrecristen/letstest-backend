{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "(.*)",
        "headers": {
          "Access-Control-Allow-Origin": "*"
        },
        "dest": "src/",
        "methods": [
          "GET",
          "POST",
          "PUT",
          "DELETE",
          "PATCH",
          "OPTIONS"
        ]
      }
    ],
    "crons": [
      {
        "path": "/api/notifications/deadline/run",
        "schedule": "0 9 * * *"
      }
    ]
  }
